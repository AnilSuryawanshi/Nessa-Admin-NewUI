.footer__bottom {
	font-size:11px;
	line-height:2.09091;
	font-family:georgia,garamond,serif;
	color:#fff !important;
	background:#00AAAD;
	overflow:hidden;
	padding:30px 0;
	position: absolute;
    width: 100%;
 }
@media all and (min-width: 768px) {
	.footer__bottom {
		font-size:13px;
		line-height:1.61538;
		padding:35px 0
	}
}

@media all and (min-width: 992px) {
	.footer__bottom {
		font-size:14px;
		padding:25px 0
	}
}
.footer__bottom a {
	color:#6d6e71
}
.footer__bottom a:hover {
	color:#c8543b
}

.container {
	margin-right:auto;
	margin-left:auto;
	padding-left:15px;
	padding-right:15px
}
.container:before,.container:after {
	content:" ";
	display:table
}
.container:after {
	clear:both
}
@media (min-width: 768px) {
	.container {
		width:750px
	}
}
@media (min-width: 992px) {
	.container {
		width:970px
	}
}
@media (min-width: 1200px) {
	.container {
		width:1170px
	}
}

.row {
	margin-left:-15px;
	margin-right:-15px
}
.row:before,.row:after {
	content:" ";
	display:table
}
.row:after {
	clear:both
}

.footer__bottomCol {
	text-align:center;
	color:#fff;
}
.footer__bottomCol--left {
	position:relative;
	float:left;
	width:100%;
	min-height:1px;
	padding-left:15px;
	padding-right:15px;
	position:relative;
	min-height:1px;
	padding-left:15px;
	padding-right:15px;
}
@media (min-width: 768px) {
	.footer__bottomCol--left {
		float:left;
		width:33.33333%
	}
}
@media (min-width: 768px) {
	.footer__bottomCol--left {
		right:33.33333%
	}
}
@media (max-width: 767px) {
	.footer__bottomCol--left {
		margin-bottom:11px
	}
}
@media all and (min-width: 768px) {
	.footer__bottomCol--left {
		text-align:left
	}
}
.footer__bottomCol--center {
	position:relative;
	float:left;
	width:100%;
	min-height:1px;
	padding-left:15px;
	padding-right:15px;
	position:relative;
	min-height:1px;
	padding-left:15px;
	padding-right:15px;
	padding-bottom:15px
}
@media (min-width: 768px) {
	.footer__bottomCol--center {
		float:left;
		width:33.33333%
	}
}
@media (min-width: 768px) {
	.footer__bottomCol--center {
		left:33.33333%
	}
}
@media all and (min-width: 768px) {
	.footer__bottomCol--center {
		padding-bottom:0
	}
}
.footer__bottomCol--right {
	position:relative;
	float:left;
	width:100%;
	min-height:1px;
	padding-left:15px;
	padding-right:15px;
	position:relative;
	min-height:1px;
	padding-left:15px;
	padding-right:15px
}
@media (min-width: 768px) {
	.footer__bottomCol--right {
		float:left;
		width:33.33333%
	}
}
@media (min-width: 768px) and (max-width: 991px) {
	.footer__bottomCol--right {
		padding-left:0
	}
}
@media(min-width:500px){
	padding-top:0;
	padding-bottom:0;
})
@media all and (min-width: 768px) {
	.footer__bottomCol--right {
		text-align:right
	}
}
.socialMedia__list {
	list-style:none;
	padding:0;
}

.socialMedia__item {
	font-size:21px;
	line-height:1;
	display:inline-block;
	list-style:none;
	padding:0 15px
}
@media (min-width: 768px) and (max-width: 991px) {
	.socialMedia__item {
		font-size:18px
	}
}

.footer__bottomCol ul.menu {
	margin:0;
	padding:0
}
.footer__bottomCol ul.menu,.footer__bottomCol li.menu-item {
	list-style:none
}
.footer__bottomCol li.menu-item {
	display:inline-block;
	padding:0 8px
}
.footer__bottomCol li.menu-item:first-of-type {
	padding-left:0
}
.footer__bottomCol li.menu-item:last-of-type {
	padding-right:0
}
.socialMedia__list li a,#menu-copyright li a{color:#fff;}